    eval "$__conda_setup"
else
    if [ -f "/Users/hyenas/opt/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/hyenas/opt/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/hyenas/opt/anaconda3/bin:$PATH"
    fi
fi
