function lshow()
{
    echo -e "\033[34m================================================================\033[0m"
    echo "      \033[34mIf this directory is git one, please git pull first.\033[0m"
    echo -e "\033[34m================================================================\033[0m"
    echo ""

    ls -alh
    
    echo ""
    echo -e "\033[34m================================================================\033[0m"
    echo "      \033[34mIf this directory is git one, please git pull first.\033[0m"
    echo -e "\033[34m================================================================\033[0m"
}

alias ll='foo(){ lshow }; foo '

